{
  "name": "sweet-sally-be/helpers",
  "description": "Various Symfony for my projects",
  "type": "library",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "keywords": [
    "extensions",
    "symfony",
    "translatable"
  ],
  "homepage": "https://www.sweetsally.be",
  "license": "unlicensed",
  "authors": [
    {
      "name": "Sweet Sally",
      "homepage": "https://www.sweetsally.be"
    }
  ],
  "require": {
    "php": ">=8.2",
    "doctrine/orm": "^3.3",
    "mailchimp/marketing": "^3.0",
    "symfony/intl": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/yaml": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.4",
    "doctrine/dbal": "^3.8",
    "symfony/var-dumper": "^6.4|^7.0",
    "doctrine/doctrine-fixtures-bundle": "^4.3",
    "fakerphp/faker": "^1.24"
  },
  "autoload": {
    "psr-4": {
      "SweetSallyBe\\Helpers\\": "src"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "drupol/composer-packages": true,
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SweetSallyBe\\Helpers\\Tests\\": "tests"
    }
  }
}
