{
  "name": "sweet-sally-be/helpers",
  "description": "Various Symfony for my projects",
  "type": "symfony-bundle",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "keywords": [
    "extensions",
    "symfony",
    "translatable"
  ],
  "homepage": "https://www.sweetsally.be",
  "license": "unlicensed",
  "authors": [
    {
      "name": "Sweet Sally",
      "homepage": "https://www.sweetsally.be"
    }
  ],
  "require": {
    "php": "8.*",
    "doctrine/doctrine-bundle": "^2.5",
    "doctrine/doctrine-migrations-bundle": "^3.0",
    "doctrine/orm": "^3.1",
    "mailchimp/marketing": "^3.0",
    "symfony/dotenv": "5.3.*|5.4.*|6.*|7.*",
    "symfony/flex": "^1.16|^2",
    "symfony/framework-bundle": "5.3.*|5.4.*|6.*|7.*",
    "symfony/intl": "5.3.*|5.4.*|6.*|7.*",
    "symfony/runtime": "5.3.*|5.4.*|6.*|7.*",
    "symfony/yaml": "5.3.*|5.4.*|6.*|7.*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "require-dev": {
    "psr/log": "^1.1",
    "symfony/browser-kit": "5.3.*|5.4.*|6.*|7.*",
    "symfony/css-selector": "5.3.*|5.4.*|6.*|7.*",
    "symfony/phpunit-bridge": "6.*|7.*",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "SweetSallyBe\\Helpers\\": "src"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/runtime": true
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SweetSallyBe\\Helpers\\Tests\\": "tests"
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.3.*|5.4.*|6.*|7.*"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
