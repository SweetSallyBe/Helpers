{
  "name": "sweet-sally-be/helpers",
  "description": "Various Symfony for my projects",
  "version": "0.082",
  "type": "library",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "keywords": [
    "extensions",
    "Symfony",
    "translatable"
  ],
  "homepage": "https://www.sweetsally.be",
  "license": "unlicensed",
  "authors": [
    {
      "name": "Sweet Sally",
      "homepage": "https://www.sweetsally.be"
    }
  ],
  "require": {
    "php": ">=8.4",
    "doctrine/orm": "^3.5",
    "mailchimp/marketing": "^3.0",
    "symfony/intl": "^7.3|^8.0",
    "symfony/property-access": "^7.3|^8.0",
    "symfony/yaml": "^7.3|^8.0"
  },
  "require-dev": {
    "doctrine/dbal": "^4.4",
    "doctrine/doctrine-bundle": "^3.1",
    "doctrine/doctrine-fixtures-bundle": "^4.3",
    "fakerphp/faker": "^1.24",
    "phpunit/phpunit": "^12.5",
    "symfony/var-dumper": "^6.4|^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "SweetSallyBe\\Helpers\\": "src"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "drupol/composer-packages": true,
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SweetSallyBe\\Helpers\\Tests\\": "tests"
    }
  }
}
